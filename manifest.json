{
    "manifest_version": 3,
    "name": "Edge 收藏夹管理插件",
    "version": "1.0",
    "description": "批量管理和编辑 Edge 收藏夹的工具",
    "permissions": ["bookmarks", "storage"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["domain-replace.html"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  